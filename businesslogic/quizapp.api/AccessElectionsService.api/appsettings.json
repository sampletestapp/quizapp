{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=Sai;Database=WISVOTE_MSCRM_EXT;Trusted_Connection=True;TrustServerCertificate=True;",
    "DataSourceConnection": "Data Source=Sai;Initial Catalog=AEWDCS;Integrated Security=True;Encrypt=True;TrustServerCertificate=True;",
    "DataTargetConnection": "Data Source=Sai;Initial Catalog=WISVOTE_MSCRM_EXT;Integrated Security=True;Encrypt=True;TrustServerCertificate=True;",
    "DataMasterConnection": "Data Source=Sai;Initial Catalog=master;Integrated Security=True;Encrypt=True;TrustServerCertificate=True;"
  },
  "RestoreInfo": {
    "Tables": "SurveyResponse,PollingPlacePhoto,ActivityLog,SurveyResponseHistory",
    "Db": "AEWDCS",
    "IdentityColumns": "PollingPlacePhotoID,ActivityLogID,SurveyResponseHistoryID"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Access Election"
    }
  },
  "AllowedHosts": "*"
}
